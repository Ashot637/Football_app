{
  "expo": {
    "name": "Arm MatchDay",
    "slug": "armmatchday",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon1024.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon1024.png"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.tesvan.ArmMatchDay"
    },
    "web": {
      "favicon": "./assets/images/icon1024.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Uploading a user photo adds a personal touch to your profile, fostering a sense of trust."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "27ea85cc-8fa8-4213-ac86-81753bd7ab09"
      }
    },
    "owner": "ashotmartirosyan"
  },
  "usesCleartextTraffic": true
}
