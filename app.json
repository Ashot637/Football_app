{
  "expo": {
    "name": "Ballhola",
    "slug": "ballhola",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon1024.png",
    "userInterfaceStyle": "light",
    "permissions": ["NOTIFICATIONS"],
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon1024.png"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.tesvan.ballhola",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/images/icon1024.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Uploading a user photo adds a personal touch to your profile, fostering a sense of trust."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "eb785808-ad7c-4a7c-b976-be600a3dd403"
      }
    }
  },
  "usesCleartextTraffic": true
}
