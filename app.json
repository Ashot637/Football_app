{
  "expo": {
    "name": "Arm MatchDay",
    "slug": "ArmMatchDay",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon1024.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon1024.png"
      },
      "permissions": ["CAMERA", "READ_EXTERNAL_STORAGE"],
      "package": "com.tesvan.ArmMatchDay"
    },
    "web": {
      "favicon": "./assets/images/icon1024.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Uploading a user photo adds a personal touch to your profile, fostering a sense of trust."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "db928bd0-898d-42f1-bc8c-8f8ffdfb7c73"
      }
    }
  },
  "usesCleartextTraffic": true
}
