{
  "expo": {
    "name": "football",
    "slug": "football",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.webp",
      "resizeMode": "cover"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.tesvan.football"
    },
    "web": {
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Uploading a user photo adds a personal touch to your profile, fostering a sense of  trust."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1d37dcd5-7c3f-44fd-b0ed-fc6c6c87820c"
      }
    }
  },
  "usesCleartextTraffic": true
}
